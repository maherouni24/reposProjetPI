         </children>
      </Pane>
      <Button fx:id="btnModifier" layoutX="14.0" layoutY="330.0" mnemonicParsing="false" onAction="#SendToProfil" text="Modifier Profil" textFill="#eeb700">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnProprietes" layoutX="189.0" layoutY="330.0" mnemonicParsing="false" onAction="#SendToProprietes" text="Propriétes" textFill="#eeb700">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnRapport" layoutX="322.0" layoutY="330.0" mnemonicParsing="false" onAction="#AddRapport" text="Rapport" textFill="#eeb700">
         <font>
            <Font name="System Bold" size="18.0" />
