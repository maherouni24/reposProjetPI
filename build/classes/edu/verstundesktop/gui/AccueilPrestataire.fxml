            <Font name="Arial Black" size="18.0" />
         </font>
      </Button>
      <ImageView fitHeight="248.0" fitWidth="307.0" layoutX="287.0" layoutY="71.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../tests/images/prestataire2.jpg" />
         </image>
         <effect>
            <Reflection bottomOpacity="1.0" fraction="0.0" topOpacity="1.0" />
         </effect>
      </ImageView>
