<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #112;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.verstundesktop.gui.AccueilPrestataireController">
   <children>
      <Label layoutX="189.0" layoutY="14.0" text="Prestataire" textFill="#eeb700">
         <font>
            <Font name="Arial" size="34.0" />
         </font>
      </Label>
      <Pane layoutX="14.0" layoutY="67.0" prefHeight="200.0" prefWidth="268.0" style="-fx-background-color: black;">
         <children>
            <Label layoutX="11.0" layoutY="23.0" text="Nom" textFill="#eeb700">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label layoutX="11.0" layoutY="91.0" text="Prenom" textFill="#eeb700">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="tfNom" editable="false" layoutX="74.0" layoutY="21.0" prefHeight="25.0" prefWidth="177.0" />
            <TextField fx:id="tfPrenom" editable="false" layoutX="74.0" layoutY="88.0" prefHeight="25.0" prefWidth="177.0" />
            <Label layoutX="12.0" layoutY="153.0" text="Mail" textFill="#eeb700">
               <font>
                  <Font name="System Bold" size="14.0" />
