<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #124;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.verstundesktop.gui.EnregistrementSiteArceologiqueController">
   <children>
      <Label layoutX="158.0" layoutY="14.0" prefHeight="38.0" prefWidth="272.0" text="Site Archeologique" textFill="#eeb700">
         <font>
            <Font name="Arial Black" size="25.0" />
         </font>
      </Label>
      <Pane layoutX="14.0" layoutY="76.0" prefHeight="230.0" prefWidth="282.0" style="-fx-background-color: black;">
         <children>
            <Label layoutX="14.0" layoutY="26.0" text="ID" textFill="#eeb700">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label layoutX="5.0" layoutY="95.0" text="Nom" textFill="#eeb700">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label layoutX="5.0" layoutY="162.0" text="Gouvernorat" textFill="#eeb700">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="tfId" layoutX="94.0" layoutY="24.0" prefHeight="25.0" prefWidth="167.0" />
            <TextField fx:id="tfNom" layoutX="94.0" layoutY="93.0" prefHeight="25.0" prefWidth="167.0" />
            <TextField fx:id="tfGouvernorat" layoutX="94.0" layoutY="160.0" prefHeight="25.0" prefWidth="167.0" />
         </children>
      </Pane>
      <TableView fx:id="table" layoutX="330.0" layoutY="76.0" onMouseClicked="#tablehandleButtonAction" prefHeight="230.0" prefWidth="256.0">
        <columns>
          <TableColumn fx:id="IdColumn" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="NomColumn" prefWidth="68.0" text="Nom" />
            <TableColumn fx:id="GouvColumn" prefWidth="112.0" text="Gouvernorat" />
        </columns>
      </TableView>
      <Button fx:id="btnAjouter" layoutX="14.0" layoutY="313.0" mnemonicParsing="false" onAction="#save" text="Ajouter" textFill="#eeb700">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="btnsupprimer" layoutX="94.0" layoutY="313.0" mnemonicParsing="false" onAction="#delete" text="supprimer" textFill="#eeb700">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnModifier" layoutX="197.0" layoutY="314.0" mnemonicParsing="false" onAction="#update" text="Modifier" textFill="#eeb700">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
